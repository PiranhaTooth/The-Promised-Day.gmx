        <arguments>
          <argument>
            <kind>1</kind>
            <string>///boss attacks and state machine
switch state
{
    case INTRO:
    {
        if introJumped = false
        {
            vspeed = -8;
            introJumped = true;
        }
        hspeed = 4;
        gravity = 0.25;
        if image_alpha &lt; 0.95
        {
            image_alpha += 0.05;
        }
    }break;
       
    case JUMPING: 
    {
        if jumped = false
        {
            if image_xscale = 1
            {
                hspeed = -4;
            }
            if image_xscale = -1
            {
                hspeed = 4;
            }
            dir = choose(-1,1)
            vspeed = 8;
            jumped = true;
            audio_play_sound(sndSlimeJump,10,false);
        }
        gravity = 0.25;
        //sprite_index = sprBoss1Jump;                    
    }break;
    
    case ATTACKING:
        if attackChosen = false
        {
            attackCurrent = choose(1,2,3)
            attackChosen = true;
        }
        if attackChosen = true
        {            
            if attackCurrent = 1
            {  
                scrMakeCircle(400,500,360,30,7,objBoss1Attack1);
                attackChosen = false;
            }
            if attackCurrent = 2
            {
                attack2timer1 += 1;
                if (attack2timer1 mod 10) = 0
                {
                    instance_create(100,100,objBoss1Attack2);
                    attack2dir1 += 14.4;
                }
                
                if (attack2timer1 mod 50) = 0
                {
                    instance_create(400,100,objBoss1Attack2_3);
                }
                
                attack2timer2 += 1;
                if (attack2timer2 mod 10) = 0
                {
                    instance_create(700,100,objBoss1Attack2_2);
                    attack2dir2 -= 14.4;
                }
                if attack2timer2 = 400
                {
                    attackChosen = false;
                }        
            }
            if attackCurrent = 3
            {
                for(var i = 0; i &lt; 5; i++)
                {
                    instance_create(32,368 + (spawnGap * i),objBoss1Attack3Left);
                    instance_create(768,368 + (spawnGap * i),objBoss1Attack3Right);
                }
                instance_create(32,524,objBoss1Attack3Summon);
                instance_create(768,524,objBoss1Attack3Summon);
                attackChosen = false;
            }
        }   
        break; 
}
</string>
